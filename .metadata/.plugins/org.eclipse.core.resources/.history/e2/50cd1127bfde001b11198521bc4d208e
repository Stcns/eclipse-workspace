package inheritance4;

class A {
	public A() {
		System.out.println("생성자 A");
	}
}

class B extends A {
	public B() {
		System.out.println("생성자 B");
	}
}

class C extends B {
	public C() {
		System.out.println("생성자 C");	// super();는 반드시 첫번째 라인에 와야함
	}
}

public class ConstructorEx02 {

	public static void main(String[] args) {
		
		C c;
		c = new C();
		
	}
	
}
