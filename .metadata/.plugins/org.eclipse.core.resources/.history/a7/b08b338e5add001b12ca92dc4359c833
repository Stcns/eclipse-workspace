package exam;

import java.util.Scanner;

public class Q14 {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		int arr[];
		int num;
		System.out.print("정수 몇개? >> ");
		num = sc.nextInt();
		
		arr = new int[num];
		for(int i = 0; i < arr.length; i++) {
			arr[i] = (int)(Math.random() * 100 + 1)
		}
		
		
	}

}
